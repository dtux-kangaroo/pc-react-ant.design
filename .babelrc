{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"]
      }
    }],
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    ["import",
      { "libraryName": "antd", "style": true }
    ],
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    "transform-runtime"
  ]
}
